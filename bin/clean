#!/bin/sh

set -E

DIR="$(cd "$(dirname "${0}")" && pwd)"
cd "${DIR}"

. "${DIR}/../etc/env/env.env"

if [ -z "${PACHYDERM_IMAGE}" ]; then
  PACHYDERM_IMAGE="${PACHYDERM_DEFAULT_IMAGE}"
fi

docker kill "${PACHYDERM_IMAGE}" > /dev/null 2> /dev/null
docker rm "${PACHYDERM_IMAGE}" > /dev/null 2> /dev/null
exit 0
