      "metadata": {
        "name": "pfsd",
        "creationTimestamp": null,
        "labels": {
          "app": "pfsd",
          "suite": "pachyderm"
        }
      },
      "spec": {
        "volumes": [
          {
            "name": "pfs-disk"
          },
          {
            "name": "modules",
            "hostPath": {
              "path": "/lib/modules"
            }
          }
        ],
        "containers": [
          {
            "name": "pfsd",
            "image": "pachyderm/pfsd",
            "ports": [
              {
                "name": "api-grpc-port",
                "containerPort": 650,
                "protocol": "TCP",
                "hostIP": "0.0.0.0"
              },
              {
                "name": "api-http-port",
                "containerPort": 750
              },
              {
                "name": "trace-port",
                "containerPort": 1050
              }
            ],
            "env": [
              {
                "name": "PFS_DRIVER_ROOT",
                "value": "/pfs/btrfs"
              },
              {
                "name": "BTRFS_DEVICE",
                "value": "/pfs-img/btrfs.img"
              },
              {
                "name": "PFS_NUM_SHARDS",
                "value": "32"
              }
            ],
            "resources": {},
            "volumeMounts": [
              {
                "name": "pfs-disk",
                "mountPath": "/pfs/btrfs"
              },
              {
                "name": "modules",
                "mountPath": "/lib/modules"
              }
            ],
            "imagePullPolicy": "",
            "securityContext": {
              "privileged": true
            }
          }
        ],
        "serviceAccountName": ""
      }
    }
  },
  "status": {
    "replicas": 0
  }
}
