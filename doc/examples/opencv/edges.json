{
  "pipeline": {
    "name": "edges"
  },
  "input": {
    "atom": {
      "glob": "/*",
      "repo": "images"
    }
  },
  "enable_stats": true,
  "transform": {
    "cmd": [
      "/bin/bash"
    ],
    "stdin": [
      "echo 'oh hai'",
      "sleep $((1 + RANDOM % 100))",
      "python3 /edges.py"
    ],
    "image": "pachyderm/opencv"
  }
}

