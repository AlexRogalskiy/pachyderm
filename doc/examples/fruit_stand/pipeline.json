{
  "pipeline": {
    "name": "filter"
  },
  "transform": {
    "cmd": [ "sh" ],
    "stdin": [
        "for fruit in apple orange banana; do",
        "   grep $fruit /pfs/d/sales | awk '{print $2}' >>/pfs/out/$fruit",
        "done"
    ]
  },
  "inputs":[
    {
      "name": "d",
      "repo": { "name": "data" },
      "branch": "master",
      "glob": "/*"
    }
  ]
}
